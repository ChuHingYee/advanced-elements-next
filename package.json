    "rollup-plugin-typescript2": "^0.31.1",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.43.5",
    "typescript": "^4.4.4",
    "vitepress": "^0.20.5",
    "vitepress-theme-demoblock": "^1.4.2"
